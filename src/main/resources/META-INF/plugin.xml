<idea-plugin>
    <id>com.pinkfloyded.idea-file-path-autocomplete.idea-file-path-autocomplete</id>
    <name>File Path Autocomplete</name>
    <vendor email="k.veneetreddy@gmail.com">PinkFloyded</vendor>

    <description><![CDATA[
    <p>Auto-completes file paths.</p>
    <i> Currently only linux is supported. Windows support and more features are under development. </i>
    ]]></description>

    <idea-version since-build="143"/>
    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <completion.contributor language="any" implementationClass="com.pinkfloyded.FilePathContributor"/>
        <completion.confidence language="any" implementationClass="com.pinkfloyded.FilePathCompletionConfidence"
                               order="before javaSkipAutopopupInStrings"/>
    </extensions>

    <actions>
    </actions>
</idea-plugin>