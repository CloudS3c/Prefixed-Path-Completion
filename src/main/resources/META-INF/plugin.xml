<idea-plugin>
    <id>Prefixed-Path-Completion</id>
    <name>File Path from Keyword Direction</name>
    <vendor email="nerux2014@gmail.com">CloudS3c</vendor>

    <description><![CDATA[
    <p>Auto-completes file path aimed from keyword.</p>
    ]]></description>

    <change-notes><![CDATA[
<ul>
    <li>Allow `/` on all platforms in paths</li>
</ul>
    ]]>
    </change-notes>
    <idea-version since-build="143"/>
    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <typedHandler implementation="com.cl0udS3c.AutoPopupTypedHandler"/>
        <completion.contributor language="any" implementationClass="com.cl0udS3c.FilePathCompletionContributor"/>
        <completion.confidence language="any" implementationClass="com.cl0udS3c.FilePathCompletionConfidence"
                               order="first"/>
    </extensions>
    <actions>
    </actions>
</idea-plugin>